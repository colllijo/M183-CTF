{
  "name": "m183-ctf-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "prestart": "npm run generate:api",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "generate:api": "ng-openapi-gen --input ../shared/openapi/openapi.json --output ./src/app/core/api",
    "generate:api-ci": "ng-openapi-gen --input ./openapi.json --output ./src/app/core/api",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "prettier": "prettier --check .",
    "prettier:write": "prettier --write ."
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.2.0",
    "@angular/cdk": "^17.3.8",
    "@angular/common": "^17.2.0",
    "@angular/compiler": "^17.2.0",
    "@angular/core": "^17.2.0",
    "@angular/forms": "^17.2.0",
    "@angular/material": "^17.3.8",
    "@angular/platform-browser": "^17.2.0",
    "@angular/platform-browser-dynamic": "^17.2.0",
    "@angular/router": "^17.2.0",
    "@ngrx/effects": "^17.2.0",
    "@ngrx/store": "^17.2.0",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "file-saver": "^2.0.5",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-builders/jest": "^17.0.0",
    "@angular-devkit/build-angular": "^17.2.3",
    "@angular-eslint/builder": "17.5.2",
    "@angular-eslint/eslint-plugin": "17.5.2",
    "@angular-eslint/eslint-plugin-template": "17.5.2",
    "@angular-eslint/schematics": "17.5.2",
    "@angular-eslint/template-parser": "17.5.2",
    "@angular/cli": "^17.2.3",
    "@angular/compiler-cli": "^17.2.0",
    "@ngrx/eslint-plugin": "^17.2.0",
    "@types/file-saver": "^2.0.7",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "7.11.0",
    "@typescript-eslint/parser": "7.11.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "ng-openapi-gen": "^0.51.0",
    "prettier": "^3.2.5",
    "typescript": "~5.3.2"
  }
}
